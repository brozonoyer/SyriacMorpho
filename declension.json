{
  "absolute":{
    "sg":{
      "m":"^(.*)$",
      "f":"^(.*)ā$"
    },
    "pl":{
      "m":"^(.*)in",
      "f":"^(.*)ān"
    }
  },
  "construct":{
    "sg":{
      "m":"^(.*)$",
      "f":"^(.*)aṯ"
    },
    "pl":{
      "m":"^(.*)ay",
      "f":"^(.*)āṯ"
    }
  },
  "emphatic":{
    "sg":{
      "m":"^(.*)ā",
      "f":["^(.*)tā", "^(.*)ṯā"]
    },
    "pl":{
      "m":"^(.*)ē",
      "f":"^(.*)āṯā"
    }
  }
}
