{
    "perfect":{
      "sg":{
        "3":{
          "m":"^(.*)$",
          "f":"^(.*)aṯ$"
        },
        "2":{
          "m":"^(.*)t$",
          "f":"^(.*)t$"
        },
        "1":"^(.*)eṯ$"
      },
      "pl":{
        "3":{
          "m":["^(.*)w$","^(.*)$","^(.*)un$"],
          "f":["^(.*)$","^(.*)y$","^(.*)ēn$"]
        },
        "2":{
          "m":"^(.*)ton$",
          "f":"^(.*)tēn$"
        },
        "1":["^(.*)n$","^(.*)nan$"]
      }
    },

    "imperfect":{
      "sg":{
        "3":{
          "m":"^n(.*)$",
          "f":"^t(.*)$"
        },
        "2":{
          "m":"^t(.*)$",
          "f":"^t(.*)in$"
        },
        "1":"^’(.*)$"
      },
      "pl":{
        "3":{
          "m":"^n(.*)un$",
          "f":"^n(.*)ān$"
        },
        "2":{
          "m":"^t(.*)un$",
          "f":"^t(.*)ān$"
        },
        "1":"^n(.*)$"
      }
    },

    "imperative":{
      "sg":{
        "m":"^(.*)$",
        "f":"^(.*)y$"
      },
      "pl":{
        "m":["^(.*)w$","^(.*)un$"],
        "f":"^(.*)$"
      }
    },

    "participle":{
      "sg":{
        "m":"^(.*)$",
        "f":"^(.*)ā$"
      },
      "pl":{
        "m":"^(.*)in$",
        "f":"^(.*)ān$"
      }
    }
}